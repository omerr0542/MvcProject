@using EntityLayer.Concrete
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Hakkımda Veri Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda Yazısı 1</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.AboutDetails1</td>
            <td><a href="#" class="btn btn-warning"> Aktif Yap </a></td>
            <td><a href="#" class="btn btn-danger"> Pasif Yap </a></td>
        </tr>
    }

</table>

<br />

@Html.Partial("AboutPartial")